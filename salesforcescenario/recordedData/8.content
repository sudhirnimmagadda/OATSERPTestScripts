		function noStorage() {
			/* Run the login rotator */
			$loginRotator = App.config.modules[0] = "login-rotator";
		}

		/* If Krux segments localstorage exists */

		if (localStorage.getItem("kxsfdc_us_allsegs")) {

			/* Store array as a variable and update eVar */
			promoSegments = localStorage.getItem("kxsfdc_us_allsegs");
			$( document ).ready(function() {
				s.eVar81= promoSegments;
			});

			/* Promotion IDs to check for */
			var $promos = [
				"rznoe6cni",
				"rznl43kvf",
				"rznlvxnu6"
			];
			var promosFound = false;

			/* If match, display associated promo */
			$.each($promos, function(index, value){
				if(promoSegments.indexOf(value) != -1) {
					$matchedPromo = value;
					$("#"+$matchedPromo).show();
					promosFound = true;
					return false;
				}
			});

			/* If no segments match promo ids run Login Rotator */
			if(!promosFound) {
				noStorage();
			}

		} else {
			/* If Krux segments localstorage does not exist */
			noStorage();
		};


