<META name=robots content="noindex, nofollow" ng-if="noIndex">
<META name={{metaTag.name}} content={{metaTag.content}} ng-repeat="metaTag in metaTags"><BASE href="/">
<SCRIPT type=text/javascript>
        var supportWindowsHistory = !!(window.history && window.history.pushState);
        // In downgraded browsers we need to use the hash in order to reach the angular routes
        if (!supportWindowsHistory) {
            if (window.location.hash.charAt(1) !== "!") {
                var baseHref = document.getElementsByTagName('base')[0].href;
                var hashRoute = location.href.replace(baseHref, "").replace(location.hash, "");
                window.location.replace(window.location.pathname + '#!' + hashRoute + window.location.search + window.location.hash);
            }
        }
    </SCRIPT>
<LINK rel="shortcut icon" href="//www.microsoft.com/favicon.ico?v2"><LINK rel=canonical ng-href="{{canonicalUrl}}"><LINK rel=alternate hreflang={{lang.locale}} ng-repeat="lang in languages" ng-href="{{canonicalUrl.replace(locale, lang.locale)}}"><LINK rel=stylesheet type=text/css href="https://assets.onestore.ms/cdnfiles/external/webcore/4.19.0/stylesheets/WebCore.4.19.0.ltr.light.min.css"></HEAD></HTML>